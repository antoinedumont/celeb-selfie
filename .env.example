# Google AI Studio API Key (required - for image generation and prompts)
# Get your key from https://aistudio.google.com/app/apikey
VITE_GOOGLE_AI_STUDIO_API_KEY=your_google_ai_studio_key_here

# Gemini API Key (alias for Google AI Studio key)
VITE_GEMINI_API_KEY=your_gemini_key_here

# CORS Proxy Configuration (for geo-blocked regions)
# Set to 'true' if Google AI API is blocked in your region
VITE_USE_CORS_PROXY=true

# US Proxy URL (primary - for bypassing geo-blocks)
VITE_US_CORS_PROXY_URL=https://us.api.tmtprod.com/google/

# Fallback Proxy URL (optional)
VITE_CORS_PROXY_URL=https://api.tmtprod.com/google/

# Cloudinary Configuration (optional - for gallery storage)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset
VITE_CLOUDINARY_API_KEY=your_api_key

# Password Protection (optional - enables simple access control)
# Leave empty or remove to disable password protection
# This is a client-side protection suitable for casual access control only
VITE_APP_PASSWORD=your_secure_password_here
